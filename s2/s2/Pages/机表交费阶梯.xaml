<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:bb="clr-namespace:Com.Aote.Behaviors"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
	xmlns:System="clr-namespace:System;assembly=mscorlib"
	mc:Ignorable="d" FontSize="13.333"
	x:Class="Com.Aote.Pages.机表交费阶梯" FontFamily="Microsoft YaHei" d:DesignWidth="800" d:DesignHeight="600">

    <Grid x:Name="LayoutRoot">
        <m:ResourceLoad x:Name="loader" Margin="0,0,-101,0">
            <!--打开窗口对象-->
            <o:ChildWindowObj Name="openxiangou" CWName="/Pages/管理员登录" />
            <o:ChildWindowObj Name="openkafei" CWName="/Pages/违约金登录" />
            <!--<o:ChildWindowObj Name="openshuiliang" CWName="/Pages/用水量登录" />-->
            <!--历史交费列表-->
            <o:ObjectList Name="jbsellgas" WebClientInfo="{StaticResource dbclient}"
				Path="{m:Exp Str=kbusers.f_userid !\= null:$from t_sellinggas where f_userinfoid\=\'\{kbfee.f_userinfoid\}\' order by f_deliverydate desc \, id desc $\,null}"
				IsOld="{m:Exp Str=kbusers.State\=\=$Loaded$}" />
            <!--付款方式列表-->
            <o:ObjectList Name="paylist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='付款方式' order by id"/>

            <!--抄表的未交费记录-->

            <!--<o:ObjectList Name="kbsellgas" LoadOnPathChanged="False" WebClientInfo="{StaticResource dbclient}"
				Path="{m:Exp Str=$from t_handplan h where  h.f_userinfoid\=\'\{kbusers.f_userid\}\' and  h.shifoujiaofei\=\'否\' and h.lastrecord is not null and h.f_state\=\'已抄表\' order by h.f_userinfoid\,h.f_handdate  $}"
				IsOld="{m:Exp Str=kbusers.State\=\=$Loaded$}"
				IsClear="{m:Exp Str=kbusers.State\=\=$LoadError$ |or| kbusers.f_userinfoid\=\=null}">
            -->
            <!--违约金天数-->
            <!--
            <o:PropertySetter PropertyName="f_zhinajintianshu"  Default="{m:Exp Str=0}"   NoDependValue="{m:Exp Str=SysTime.Today.Subtract(this.f_endjfdate.ToDate()).Days\>0:SysTime.Today.Subtract(this.f_endjfdate.ToDate()).Days\,0}"/>
            -->
            <!--违约金-->
            <!--
            <o:PropertySetter PropertyName="f_zhinajin"   Default="{m:Exp Str=0}"  
					NoDependValue="{m:Exp Str=(this.f_fee*this.f_zhinajintianshu*(kbusers.f_usertype\=\=$民用$:single[data.name\=\=$民用违约金比率$].ToObjectList().First().value.ToDouble()\,kbusers.f_usertype\=\=$非民用$:single[data.name\=\=$非民用违约金比率$].ToObjectList().First().value.ToDouble()\,null)).MathRound(2)}"/>
            -->
            <!--抄表的未交费记录-->
            <o:ObjectList Name="kbsellgas" LoadOnPathChanged="False" WebClientInfo="{StaticResource dbclient}">
                <!--减免用水量-->
                <o:PropertySetter PropertyName="f_jianshuiliang"   Default="{m:Exp Str=0}"  
					NoDependValue="{m:Exp Str=(this.lastrecord-this.lastinputgasnum-this.oughtamount)}"/>
            </o:ObjectList>

            <!--选中的对象列表-->
            <o:ObjectList Name="selected" EntityType="kbsellgas"
			Source="{m:Exp Str=dataGrid1\=>SelectionChanged!\=null:kbsellgas[data.Pos(kbsellgas)^\=dataGrid1.SelectedItem.Pos(kbsellgas)].ToObjectList()\,null}"/>
            <!--金额合计-->
            <o:GeneralObject Name="heji" EntityType="t_sellinggas" WebClientInfo="{StaticResource dbclient}"
				Path="{m:Exp Str=userfiles.SelectedItem.f_userid !\= null:$one/select new Map(Sum(f_grossproceeds) as f_grossproceeds) from t_sellinggas where f_userid \=\{userfiles.SelectedItem.f_userid\}$\,$0$}"
				IsOld="{m:Exp Str=heji.State\=\=$End$}" >
            </o:GeneralObject>
            <!--机表用户对象-->
            <o:GeneralObject Name="kbusers" NotEmpty="True" LoadOnPathChanged="True" EntityType="t_userinfo" WebClientInfo="{StaticResource dbclient}"
				Path="{m:Exp Str=kbfee1.DataContext.f_userinfoid!\=null:$one/from t_userinfo u  where u.f_userid\=\'\{kbfee1.DataContext.f_userinfoid\}\'$\,$null$}" 
				
			 	IsInit="{m:Exp Str=kbusers.State\=\=$LoadError$}">
            </o:GeneralObject>

            <!--机表收费对象-->
            <o:GeneralObject Name="kbfee" EntityType="t_sellinggas" WebClientInfo="{StaticResource dbclient}"
            IsInit="{m:Exp Str=
			nouser\=>OK!\=null |or|
			cancle\=>Click!\=null |or| 
			print.State\=\=$End$ or print.State\=\=$Cancle$}">
                <!--上期指数-->
                <o:PropertySetter PropertyName="lastinputgasnum" NoDependValue="{m:Exp Str=kbsellgas.First().lastinputgasnum\=>DataLoaded}"/>
                <!--本期指数-->
                <o:PropertySetter PropertyName="lastrecord" NoDependValue="{m:Exp Str=dataGrid1.SelectedItem.lastrecord}"/>
                <!--违约金-->
                <o:PropertySetter PropertyName="f_zhinajin1" NoDependValue="{m:Exp Str=selected[True].Sum(f_zhinajin)}" Default="0"/>
                <o:PropertySetter PropertyName="f_zhinajin" NoDependValue="{m:Exp Str='openkafei.Result!=null:openkafei.Result\,kbfee.f_zhinajin1!=null:kbfee.f_zhinajin1\,0'}" Default="0"/>
                <!--减免水量-->
                <!--<o:PropertySetter PropertyName="f_jianshuiliang" NoDependValue="{m:Exp Str=selected[True].Sum(f_jianshuiliang)}" Default="0"/>-->
                <!--抄表记录id号-->
                <o:PropertySetter PropertyName="f_useful" NoDependValue="{m:Exp Str=selected[True].ToString(id)}"/>
                <!--起始抄表日期-->
                <o:PropertySetter PropertyName="lastinputdate" NoDependValue="{m:Exp Str=kbsellgas.First().scinputdate\=>DataLoaded}" />
                <!--维护费单价-->
                <o:PropertySetter PropertyName="f_unitfee" NoDependValue="{m:Exp Str=(kbfee.f_usertype\=\=$民用$ and kbfee.f_dibaohu!\=$True$):single[data.name\=\=$民用月维护费$].ToObjectList().First().value\,(kbfee.f_usertype\=\=$民用$ and kbfee.f_dibaohu\=\=$True$):single[data.name\=\=$民用低保户月维护费$].ToObjectList().First().value\,0}"/>
                <!--维护费金额  交费月数*单价-->
                <o:PropertySetter PropertyName="f_amountmaintenance" 
					NoDependValue="{m:Exp Str=this.f_usertype\=\=$民用$ and this.f_endfee!\=null:kbfee.f_repairnum*kbfee.f_unitfee\,this.f_usertype\=\=$非民用$ and this.f_endfee!\=null:kbfee.f_repairnum*kbfee.f_feiweihufei\,0}"/>
                <!--非民用计算回写月个数-->
                <o:PropertySetter PropertyName="f_repairnumfei"  
					NoDependValue="{m:Exp Str=kbfee.f_usertype\=\=$非民用$:kbfee.f_repairnum.ToInt()*12\,0}" />
                <!--计算维护费的中间变量-->
                <o:PropertySetter PropertyName="f_beginfeebj" NoDependValue="{m:Exp Str=kbfee.f_beginfee !\= null:(SysTime.Today>kbfee.f_beginfee.ToDate():kbfee.f_beginfee.ToDate()\,null)\,null}"/>
                <!--计算出的维护费交费年数-->
                <o:PropertySetter PropertyName="f_weihunians" NoDependValue="{m:Exp Str= kbfee.f_beginfee !\=null:(SysTime.Today.Subtract(kbfee.f_beginfee.ToDate()).Days >0:SysTime.Today.Subtract(kbfee.f_beginfee.ToDate()).Days/365\,0)\,0}"/>
                <!--计算出的维护费交费月数小计-->
                <o:PropertySetter PropertyName="f_weihuheji" NoDependValue="{m:Exp Str=kbfee.f_weihunians!\=null and kbfee.f_weihuyue!\=null:(kbfee.f_weihunians.ToInt()+kbfee.f_weihuyue.ToInt())*12-1\,0}"/>
                <!--计算出的维护费交费月数-->
                <o:PropertySetter PropertyName="f_weihuyue" NoDependValue="{m:Exp Str= kbfee.f_beginfee !\=null:((kbfee.f_beginfee.ToDate().Year\=\=SysTime.Today.Year and kbfee.f_beginfee.ToDate().Month>SysTime.Today.Month):0\,1)\,0}"/>
                <!--非民用维护费年费-->
                <o:PropertySetter PropertyName="f_feiweihufei" NoDependValue="{m:Exp Str=kbusers.f_feiweihufei}" />
                <!--维修费回写-->
                <o:PropertySetter PropertyName="f_beginfeehx" NoDependValue="{m:Exp Str='(kbfee.f_beginfee !\=null and kbfee.f_usertype\=\=$民用$):kbfee.f_beginfee.ToDate().AddMonths(kbfee.f_repairnum.ToInt())\,(kbfee.f_beginfee !\=null and kbfee.f_usertype\=\=$非民用$):kbfee.f_beginfee.ToDate().AddMonths(kbfee.f_repairnumfei.ToInt())\,kbfee.f_beginfee'}"/>
                <!--本次总累计购水量 预购水量+上次总累计购水量-->
                <o:PropertySetter PropertyName="f_cumulativepurchase" NoDependValue="{m:Exp Str=kbfee.f_upbuynum+kbfee.f_pregas}"/>
                <!--本次表累计购水量 预购水量+表上次总累计购水量-->
                <o:PropertySetter PropertyName="f_metergasnums" NoDependValue="{m:Exp Str=kbfee.f_premetergasnums+kbfee.f_pregas}" />
                <!--表号-->
                <o:PropertySetter PropertyName="f_meternumber" NoDependValue="{m:Exp Str=kbusers.f_meternumber}"/>
                <!--是否结账-->
                <o:PropertySetter PropertyName="f_jiezhangstate" Default="未结账"/>
                <!--交费类型-->
                <o:PropertySetter PropertyName="f_payfeetype" Default="机表收费"/>
                <!--购水有效类型-->
                <o:PropertySetter PropertyName="f_payfeevalid" Default="有效"/>
                <!--最后购水量_冲正时用-->
                <o:PropertySetter PropertyName="f_finallybought" NoDependValue="{m:Exp Str=kbusers.f_finallybought}"  />
                <!--最后购水量日期_冲正时用-->
                <o:PropertySetter PropertyName="f_finabuygasdate" NoDependValue="{m:Exp Str=kbusers.f_finabuygasdate}"  />
                <!--分公司编号-->
                <o:PropertySetter PropertyName="f_fengongsinum" Default="{m:Exp Str=LoginUser.f_fengongsinum}"/>
                <!--公司类型，分为水公司、银行 -->
                <o:PropertySetter PropertyName="f_comtype" Default="自来水水公司" Value="自来水水公司" NotNull="True"/>
            </o:GeneralObject>

            <!--发票对象-->
            <o:GeneralObject Name="fpinfosobj" EntityType="t_fapiaoinfos" WebClientInfo="{StaticResource dbclient}" 
			Path="{m:Exp Str=$one/from t_fapiaoinfos where f_invoicenum\=\'\{kbfee1.DataContext.f_invoicenum\}\'}"  
			IsInit="{m:Exp Str=print.State\=\=$End$ |or| print1.State\=\=$End$}">
                <o:PropertySetter PropertyName="f_userid" Value="{m:Exp Str=kbfee1.DataContext.f_userid}" />
            </o:GeneralObject>


            <!--打印完成后，重新获取发票号-->
            <b:ScriptAction Name="loadfapiao" CanRun="{m:Exp Str=print.State\=\=$End$ |or| print1.State\=\=$End$}" Start="{m:Exp Str=LoginUserfapiao.IsOld\=True}"></b:ScriptAction>
            <o:SearchObject Name="search"/>

            <!--用户列表-->
            <o:PagedObjectList Name="userfileslist" WebClientInfo="{StaticResource dbclient}"
		       Path="{m:Exp Str=$from t_userinfo where f_userstate\=\'正常\' and \{search.Condition\}}"
		       PageIndex="{m:Exp Str=pager.PageIndex}" PageSize="{m:Exp Str=pager.PageSize}" SumNames="id"/>
        </m:ResourceLoad>
        <Grid >
            <Grid.RowDefinitions>
                <RowDefinition Height="0.7*"/>
                <RowDefinition Height="0.3*"/>
            </Grid.RowDefinitions>
            <Grid x:Name="LayoutRoot1" Background="White" d:LayoutOverrides="Width">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="0.35*"/>
                    <ColumnDefinition Width="0.65*"/>
                </Grid.ColumnDefinitions>
                <c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=userfiles.ItemsSource.IsBusy}" Grid.Column="0" Margin="0" Grid.RowSpan="1" >
                    <toolkit:DockPanel>
                        <sdk:DataPager x:Name="pager" Source="{Binding ItemsSource.Count, Converter={StaticResource DataPagerConverter}, ElementName=userfiles}" PageSize="20" VerticalAlignment="Top" FontSize="13.333" toolkit:DockPanel.Dock="Bottom"/>
                        <toolkit:DockPanel  x:Name="userfilesearch" DataContext="{m:Exp Str=search}" toolkit:DockPanel.Dock="Top" Margin="0,0,0,5" Background="#FFCFEFFF"  >
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="28"/>
                                    <RowDefinition Height="28"/>
                                    <RowDefinition Height="28"/>
                                    <RowDefinition Height="28"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition Width="50*"/>
                                    <ColumnDefinition Width="50*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock TextWrapping="Wrap" Text="用户编号：" Margin="5,4,0,4" Height="25" FontSize="13.333" HorizontalAlignment="Right" />
                                <TextBox TextWrapping="Wrap" Grid.Column="1" Text="{m:Bind Path=f_userid}" Width="Auto" Margin="0" Height="25" FontSize="13.333" >
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_userid"  Operator="f_userid = 'this'"/>
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBlock TextWrapping="Wrap" Grid.Row="1" Text="用户姓名：" Margin="5,4,0,4" Height="25" FontSize="13.333" HorizontalAlignment="Right" />
                                <TextBox TextWrapping="Wrap" Grid.Row="1" Grid.Column="1" Text="{m:Bind Path=f_username}" Width="Auto" Margin="0" Height="25" FontSize="13.333" >
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_username"  Operator="f_username like 'this%'"/>
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBlock TextWrapping="Wrap" Grid.Row="3" Text="电　　话：" Margin="5,4,0,4" Height="25" FontSize="13.333" HorizontalAlignment="Right" />
                                <TextBox TextWrapping="Wrap" Grid.Row="3" Grid.Column="1" Text="{m:Bind Path=f_phone}" Width="Auto" Margin="0" Height="25" FontSize="13.333" >
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_phone"  Operator="f_phone like 'this%'"/>
                                    </a:Data.Prop>
                                </TextBox>
                                <Button Content="查  询" x:Name="searchbutton" Grid.Row="2" Grid.Column="2" Click="{m:Exp Str=search.Search()}" Margin="0,1,5,1" HorizontalAlignment="Right" Width="70"  FontSize="13.333"/>
                                <TextBlock TextWrapping="Wrap" Grid.Row="2" Text="地　　址：" Margin="0,5,0,-2" Height="25" FontSize="13.333" HorizontalAlignment="Right" />
                                <TextBox TextWrapping="Wrap" Grid.Row="2" Grid.Column="1" Text="{m:Bind Path=f_address}" Width="Auto" Margin="0,3,0,0" Height="25" FontSize="13.333" >
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_address"  Operator="f_address like '%this%'"/>
                                    </a:Data.Prop>
                                </TextBox>
                            </Grid>
                        </toolkit:DockPanel>
                        <sdk:DataGrid x:Name="userfiles" Margin="0"  FontSize="13.333" ItemsSource="{m:Exp Str=userfileslist}"
							SelectionChanged="userfiles_SelectionChanged" 
							AutoGenerateColumns="False" Width="Auto" Background="{x:Null}" IsReadOnly="True" >
                            <sdk:DataGrid.Columns>
                                <sdk:DataGridTextColumn d:IsLocked="True" Header="序  号" Binding="{Binding Index}"/>
                                <sdk:DataGridTextColumn Header="用户编号" Binding="{Binding f_userid}"/>
                                <sdk:DataGridTextColumn d:IsLocked="True" Header="用户姓名" Binding="{Binding f_username}"/>
                                <sdk:DataGridTextColumn Header="地址" Binding="{Binding f_address}"/>
                                <sdk:DataGridTextColumn Header="用户状态" Binding="{Binding f_userstate}"/>
                                <sdk:DataGridTextColumn Header="用户类型" Binding="{Binding f_usertype}"/>
                                <sdk:DataGridTextColumn Header="小区名称" Binding="{Binding f_districtname}"/>
                                <sdk:DataGridTextColumn Header="门牌号" Binding="{Binding f_apartment}"/>
                                <sdk:DataGridTextColumn Header="电  话" Binding="{Binding f_phone}"/>
                                <sdk:DataGridTextColumn Header="用水性质" Binding="{Binding f_gasproperties}"/>
                                <sdk:DataGridTextColumn Header="水价类型" Binding="{Binding f_stairtype}"/>
                                <sdk:DataGridTextColumn Header="账户余额" Binding="{Binding f_zhye}"/>
                                <sdk:DataGridTextColumn Header="总购水量" Binding="{Binding f_cumulativepurchase}"/>
                                <sdk:DataGridTextColumn Header="当前表购水量" Binding="{Binding f_metergasnums}"/>
                                <sdk:DataGridTextColumn Header="本次剩余补水量" Binding="{Binding f_thisnextaddgasamount}"/>
                                <sdk:DataGridTextColumn Header="维护费日期" Binding="{Binding f_beginfee, StringFormat=yyyy-MM}"/>
                                <sdk:DataGridTextColumn Header="发卡日期" Binding="{Binding f_givecarddate, StringFormat=yyyy-MM-dd}"/>
                                <sdk:DataGridTextColumn Header="是否发卡" Binding="{Binding f_whethergivecard}"/>
                                <sdk:DataGridTextColumn Header="最后购水量" Binding="{Binding f_finallybought}"/>
                                <sdk:DataGridTextColumn Header="最后购水日期" Binding="{Binding f_finabuygasdate, StringFormat=yyyy-MM-dd}"/>
                                <sdk:DataGridTextColumn Header="表安装日期" Binding="{Binding f_watchinstalldate, StringFormat=yyyy-MM-dd}"/>
                                <sdk:DataGridTextColumn Header="所属公司" Binding="{Binding f_filiale}"/>
                                <sdk:DataGridTextColumn Header="网点" Binding="{Binding f_yytdepa}"/>
                                <sdk:DataGridTextColumn Header="建档人" Binding="{Binding f_yytoper}"/>
                                <sdk:DataGridTextColumn Header="建档日期" Binding="{Binding f_yytdate, StringFormat=yyyy-MM-dd}"/>
                                <sdk:DataGridTextColumn Header="时间" Binding="{Binding f_yyttime, StringFormat=hh:mm:ss tt}"/>
                            </sdk:DataGrid.Columns>
                        </sdk:DataGrid>
                        <!--提交按钮-->
                    </toolkit:DockPanel>
                </c:BusyIndicatorEx>
                <Grid Grid.Column="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="0.9*"/>
                        <RowDefinition Height="0.1*"/>
                    </Grid.RowDefinitions>
                    <ScrollViewer toolkit:DockPanel.Dock="Top" Grid.Column="1" VerticalAlignment="Top" >
                        <StackPanel toolkit:DockPanel.Dock="Top">
                            <Grid   Margin="0"  x:Name="kbfee1" DataContext="{m:Exp Str=kbfee}" >
                                <Grid.Background>
                                    <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                                        <GradientStop Color="#FFF2F8FF" Offset="0"/>
                                        <GradientStop Color="#FFDDF2FD" Offset="1"/>
                                    </LinearGradientBrush>
                                </Grid.Background>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="1"/>
                                    <RowDefinition Height="10"/>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="10"/>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="33"/>
                                    <RowDefinition Height="33"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="130"/>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="130"/>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="130"/>
                                    <ColumnDefinition Width="1*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="用户编号：" Margin="-5,4,5,4" TextWrapping="Wrap"  TextAlignment="Right" Height="25"/>
                                <TextBlock Text="用户/单位名称：" Margin="0" TextWrapping="Wrap"  TextAlignment="Right" Grid.Column="2" Height="25"/>
                                <TextBlock Text="地　　址：" Margin="0" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Column="2" RenderTransformOrigin="1.638,0.44"/>
                                <TextBlock Text="小区名称：" Margin="0" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                                <TextBlock Margin="0" Grid.Row="3" TextWrapping="Wrap" Text="水价类型：" TextAlignment="Right" Grid.Column="2" Height="25"/>
                                <TextBlock Text="上次总累计购水量："  TextWrapping="Wrap" TextAlignment="Right" Grid.Row="6" Visibility="Collapsed" Margin="0" Height="25"/>
                                <TextBlock Text="用户类型：" Margin="0" Grid.Row="3" TextWrapping="Wrap" TextAlignment="Right" Grid.Column="4" Height="25" HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                <TextBlock Text="用水性质：" Margin="0" Grid.Row="3" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                                <TextBlock Text="维管费起始月份：" Margin="0" Grid.Row="7" TextWrapping="Wrap" TextAlignment="Right" Visibility="Collapsed" Height="25"/>
                                <TextBlock Text="维管费截止月份：" Margin="0" Grid.Row="7" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Column="2" Visibility="Collapsed"/>
                                <TextBlock Text="用 水 量：" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="6" Height="25" Grid.Column="4"/>
                                <TextBlock Text="基础水费：" Margin="0,6,48,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="7" Grid.Column="4" Height="25" HorizontalAlignment="Right" Width="72"/>
                                <TextBlock Text="附加费用：" Grid.Row="7" Grid.Column="5" Margin="10,6,8,4"/>
                                <TextBlock Text="付款方式：" Margin="0" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="8" Height="25"/>
                                <TextBlock Text="收　　款：" Grid.Column="2" Margin="0" Grid.Row="8" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                                <TextBlock Text="应交金额：" Margin="0,6" Grid.Row="8" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Column="4"/>
                                <TextBlock Text="本期结余：" Margin="0,5,0,7" Grid.Row="7" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                                <Border  Grid.Row="5"  BorderThickness="0,0,0,3"  Grid.ColumnSpan="6" >
                                    <Border.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFC6E2FB" Offset="1"/>
                                            <GradientStop Color="#FFE6F1FB"/>
                                        </LinearGradientBrush>
                                    </Border.Background>
                                </Border>
