	x:Class="Com.Aote.Pages.用水量减免" FontSize="13">

    <Grid x:Name="LayoutRoot" Background="White">
        <m:ResourceLoad x:Name="loader" Margin="0">
            <o:PagedObjectList Name="personlist" WebClientInfo="{StaticResource dbclient}" 
                    Path="{m:Exp Str=$from t_handplan where \{searchHandUserUnit.DataContext.Condition\} }"
					IsOld="{m:Exp Str=SaveAction.State\=\=$End$}"
					 PageIndex="{m:Exp Str=pager.PageIndex}" PageSize="{m:Exp Str=pager.PageSize}"
					 SumNames=","/>
            <!--更改对象-->
            <o:GeneralObject Name="scbdw" WebClientInfo="{StaticResource dbclient}"   EntityType="t_yongshuiliang"
				  IsInit="{m:Exp Str=SaveAction.State\=\=$End$}" Source="{m:Exp Str=handUserUnits.SelectedItem}"/>
            <!-- 更新抄表对象 -->
            <b:HQLAction Name="updateHandplan" WebClientInfo="{StaticResource server}">
                <b:HQLAction.HQL>
                    <m:Exp Str="$update t_handplan set oughtfee={scbdw.oughtfee},
                f_operator='{scbdw.f_operator}', f_inputtor='{scbdw.f_inputtor}',f_stair1amount='{scbdw.f_stair1amount}',f_stair2amount='{scbdw.f_stair2amount}',f_stair3amount='{scbdw.f_stair3amount}',f_stair4amount='{scbdw.f_stair4amount}',
                f_stair1fee='{scbdw.f_stair1fee}',f_stair2fee='{scbdw.f_stair2fee}',f_stair3fee='{scbdw.f_stair3fee}',f_stair4fee='{scbdw.f_stair4fee}',
                f_stair1price='{scbdw.f_stair1price}',f_stair2price='{scbdw.f_stair2price}',f_stair3price='{scbdw.f_stair3price}',f_stair4price='{scbdw.f_stair4price}',
                oughtamount={scbdw.oughtamount},extrazjfee='{scbdw.extrazjfee}',f_fee='{scbdw.f_fee}' where id={scbdw.id}">
                    </m:Exp>
                </b:HQLAction.HQL>
            </b:HQLAction>
            <!-- 更新附加费用表信息 -->
            <b:HQLAction Name="updateExtraprices" WebClientInfo="{StaticResource server}">
                <b:HQLAction.HQL>
                    <m:Exp Str="$update t_extraprices set f_pregas={scbdw.oughtamount},f_extrafee={scbdw.oughtamount}*f_extraprices where parentid={scbdw.id}">
                    </m:Exp>
                </b:HQLAction.HQL>
            </b:HQLAction>
            <!-- 更新用户档案, 抄表记录已交：用户余额=当前余额+原扣除额-现扣除额 -->
            <b:HQLAction Name="updateUserfile" WebClientInfo="{StaticResource server}">
                <b:HQLAction.HQL>
                    <m:Exp Str="
                $update t_userfiles set lastinputgasnum={scbdw.lastrecord}, 
                f_zhye={scbdw.shifoujiaofei==$是$:$f_zhye+{handUserUnits.SelectedItem.f_fee}-{scbdw.f_fee}$, $f_zhye$} 
                where f_userid={scbdw.f_userid}">
                    </m:Exp>
                </b:HQLAction.HQL>
            </b:HQLAction>

            <!--批处理-->
            <b:BatchExcuteAction Name="SaveAction"  WebClientInfo="{StaticResource dbclient}">
                <!--抄表记录保存-->
                <b:BatchInfo Source="{m:Exp Str=updateHandplan}" MethodName="InvokeToJson"/>
                <!--用户档案更新-->
                <b:BatchInfo Source="{m:Exp Str=updateUserfile}" MethodName="InvokeToJson"/>
                <!-- 更新附加费用表信息 -->
                <b:BatchInfo Source="{m:Exp Str=updateExtraprices}" MethodName="InvokeToJson"/>
            </b:BatchExcuteAction>
        </m:ResourceLoad>
