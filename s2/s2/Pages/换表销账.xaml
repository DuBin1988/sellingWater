<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
	xmlns:oo="clr-namespace:Com.Aote.ObjectTools"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
    x:Class="Com.Aote.Pages.换表销账" FontSize="13"
    mc:Ignorable="d"  FontFamily="Microsoft YaHei">
    <Grid x:Name="LayoutRoot">
        <m:ResourceLoad Margin="0,0,-12,0" x:Name="loader">
            <!--卡号获取-->
            <o:GeneralObject Name="cardid1" EntityType="t_cardreset" WebClientInfo="{StaticResource dbclient}"
           	LoadOnPathChanged="False"   
		   	Path="{m:Exp Str=userfiles.f_usertype\=\=$民用$:$sernum/from t_cardreset where id \=(select parentid from cardresetwatch where f_gaswatchbrand\=\'\{userfiles.f_gaswatchbrand\}\' and f_fengongsi\=\'\{LoginUser.f_fengongsi\}\' and f_minyong\=\'民用\' and parentid is not null)/f_qishicard$\,userfiles.f_usertype\=\=$非民用$:$sernum/from t_cardreset where id \=(select parentid from cardresetwatch where f_gaswatchbrand\=\'\{userfiles.f_gaswatchbrand\}\' and f_fengongsi\=\'\{LoginUser.f_fengongsi\}\' and f_feiminyong\=\'非民用\' and parentid is not null)/f_qishicard$\,null}">
            </o:GeneralObject>

            <!--水表品牌列表-->
            <o:ObjectList Name="pinpailist" WebClientInfo="{StaticResource dbclient}" Path="{m:Exp Str=$from qibiao as qibiao left join fetch qibiao.qbnumber}"/>

            <!--换表对象-->
            <o:GeneralObject Name="pipelinego" EntityType="t_changmeter" WebClientInfo="{StaticResource dbclient}">
            </o:GeneralObject>
            <!--更改换表对象-->
            <b:HQLAction Name="xiaozhang" WebClientInfo="{StaticResource server}">
                <b:HQLAction.HQL>
                    <m:Exp Str="
                $update t_changmeter set f_state='换表销账', f_dengjistate='申请已处理',
                          f_cmnewgaswatchbrand='{pipelinego.f_cmnewgaswatchbrand}',f_newgasmetermanufacturers='{pipelinego.f_newgasmetermanufacturers}', 
                          f_cmnewmetertype='{pipelinego.f_cmnewmetertype}', f_newgasmeterkoujing='{pipelinego.f_newgasmeterkoujing}',
                          f_newgasmeterstyle='{pipelinego.f_newgasmeterstyle}',f_cmnewmeternumber='{pipelinego.f_cmnewmeternumber}',
                          f_newweizhi='{pipelinego.f_newweizhi}',f_newgasmeteraccomodations='{pipelinego.f_newgasmeteraccomodations}',
                          f_oldcurrentbase='{pipelinego.f_oldcurrentbase}' where f_changemeterid='{pipelinego.f_changemeterid}'">
                    </m:Exp>
                </b:HQLAction.HQL>
            </b:HQLAction>
            <!-- 获取标记信息 -->
            <o:GeneralObject Name="tabletel" EntityType="t_table_tel" WebClientInfo="{StaticResource dbclient}"
                             IsInit="{m:Exp Str=SaveAction.State\=\=$End$}"
                             Path="{m:Exp Str=$one/select new Map(meter_phone as meter_phone\,terminal_name as terminal_name) from t_table_tel  where f_meternumber\=\'\{pipeline.DataContext.f_cmnewmeternumber\}\' $}">
            </o:GeneralObject>
            <!--换表申请状态列表-->
            <o:PagedObjectList Name="pipelineslist" WebClientInfo="{StaticResource dbclient}" IsOld="{m:Exp Str=SaveAction.State\=\=$End$}"
            Path="{m:Exp Str=$from t_changmeter where \{pipelinesearch.DataContext.Condition\} and f_state\=\'换表申请\' and f_dengjistate\=\'申请未处理\' and f_filiale \=\'\{LoginUser.f_fengongsi\}\'}"
			PageIndex="{m:Exp Str=pager.PageIndex}" PageSize="{m:Exp Str=pager.PageSize}" SumNames="f_times"/>
            <!--用户对象-->
            <b:HQLAction Name="updateuserfiles" WebClientInfo="{StaticResource server}">
                <b:HQLAction.HQL>
                    <m:Exp Str="
                $update t_userfiles set  f_gaswatchbrand='{pipelinego.f_cmnewgaswatchbrand}',f_gasmetermanufacturers='{pipelinego.f_newgasmetermanufacturers}', 
                          f_metertype='{pipelinego.f_cmnewmetertype}', f_gasmeterkoujing='{pipelinego.f_newgasmeterkoujing}',
                          f_gasmeterstyle='{pipelinego.f_newgasmeterstyle}',f_meternumber='{pipelinego.f_cmnewmeternumber}',
                          f_weizhi='{pipelinego.f_newweizhi}',lastinputgasnum='{pipelinego.f_newgasmeteraccomodations}',f_channel_type='{pipelinego.f_channel_type}',
                          switch_op='{pipelinego.switch_op}',meter_phone='{pipelinego.meter_phone}',terminal_name='{pipelinego.terminal_name}',
                          refreshCache='1',f_jllx='{pipelinego.f_jllx}',f_flownum='{pipelinego.f_flownum}',f_aliasname='{pipelinego.f_aliasname}'
                          where f_userid='{pipelinego.f_userid}'">
                    </m:Exp>
                </b:HQLAction.HQL>
            </b:HQLAction>
            <o:GeneralObject Name="userfiles" NotEmpty="true" EntityType="t_userfiles" WebClientInfo="{StaticResource dbclient}"
            Path="{m:Exp Str=card.CardId\=>ReadCompleted!\=null and card.Factory!\=null:$one/from t_userfiles where f_cardid\=\'\{card.CardId\}\' and f_aliasname\=\'\{card.Factory\}\' and f_filiale\=\'\{LoginUser.f_fengongsi\}\'$\,pipelines.SelectedItem.f_userid\=>SelectionChanged\!\=null:$one/from t_userfiles where f_userid\=\'\{pipelines.SelectedItem.f_userid\}\' and f_filiale\=\'\{LoginUser.f_fengongsi\}\'$\,$null$}">
                <!--换表回写-->
                <!--
                <o:PropertySetter PropertyName="f_gaswatchbrand" Value="{m:Exp Str=pipeline.DataContext.f_cmnewgaswatchbrand}"/>
                <o:PropertySetter PropertyName="f_metertype" Value="{m:Exp Str=pipeline.DataContext.f_cmnewmetertype}"/>
                <o:PropertySetter PropertyName="f_gasmeterkoujing" Value="{m:Exp Str=pipeline.DataContext.f_newgasmeterkoujing}"/>
                <o:PropertySetter PropertyName="f_meternumber" Value="{m:Exp Str=pipeline.DataContext.f_cmnewmeternumber}"/>
                <o:PropertySetter PropertyName="f_gasmetermanufacturers" Value="{m:Exp Str=pipeline.DataContext.f_newgasmetermanufacturers}"/>
                <o:PropertySetter PropertyName="f_gasmeterstyle" Value="{m:Exp Str=pipeline.DataContext.f_newgasmeterstyle}"/>
                <o:PropertySetter PropertyName="f_cardid" Value="{m:Exp Str='cardid1.State \=\= $Loaded$:cardid1.f_qishicard,null'}" />
                <o:PropertySetter PropertyName="f_metertitles" Value="{m:Exp Str=pipeline.DataContext.f_cmnewmeternumber}"/>
                <o:PropertySetter PropertyName="f_metergasnums" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:0\,0}"/>
                <o:PropertySetter PropertyName="f_cypregas" Value="{m:Exp Str=pipeline.DataContext.f_cypregas}"/>
                <o:PropertySetter PropertyName="f_weizhi" Value="{m:Exp Str=pipeline.DataContext.f_newweizhi}"/>
                -->
                <!--卡信息-->
                <!--
                <o:PropertySetter PropertyName="klx" Value="{m:Exp Str=pipeline.DataContext.klx}"/>
                <o:PropertySetter PropertyName="dqdm" Value="{m:Exp Str=pipeline.DataContext.dqdm}"/>
                <o:PropertySetter PropertyName="yhh" Value="{m:Exp Str=pipeline.DataContext.yhh}"/>
                <o:PropertySetter PropertyName="bjql" Value="{m:Exp Str=pipeline.DataContext.bjql}"/>
                <o:PropertySetter PropertyName="czsx" Value="{m:Exp Str=pipeline.DataContext.czsx}"/>
                <o:PropertySetter PropertyName="tzed" Value="{m:Exp Str=pipeline.DataContext.tzed}"/>
                <o:PropertySetter PropertyName="kmm" Value="{m:Exp Str=pipeline.DataContext.kmm}"/>
                -->
                <!--换机表时，抄表指数为新表底数-->
                <!--
                <o:PropertySetter PropertyName="lastinputgasnum" 
				NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:pipelinego.f_newgasmeteraccomodations\,null}"/>
                -->
                <!--水表型号对应类型-->
                <!--
                <o:PropertySetter PropertyName="f_type" Value="{m:Exp Str=pipeline.DataContext.f_newtype}"/>
                <o:PropertySetter PropertyName="f_zherownum" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:pipelinego.f_zherownum.ToInt()\,0}"/>
                -->
                <!--初始化卡标记-->
                <!--
                <o:PropertySetter PropertyName="f_initcardsellgas" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:$初始化卡购水标记$\,null}"/>-->
            </o:GeneralObject>

            <!--卡对象-->
            <oo:NewGeneralICCard Name="card"    
		     CanInitCard="{m:Exp Str='cardid1.State\=>DataLoaded\=\=$Loaded$'}"
		     Gas="0"
		     CardId="{m:Exp Str=pipelinego.f_newcardid}"
		     Factory="{m:Exp Str=pipelinego.f_aliasname}"
		     BuyTimes="1" 
		     Klx="{m:Exp Str='pipelinego != null\:pipelinego.klx.ToInt()\,null'}"		
		     Kzt="{m:Exp Str=cardid1\=>DataLoaded!\=null:0\,0}"
		     Dqdm="{m:Exp Str='pipelinego != null \:pipelinego.dqdm\,null'}"
		     Yhh="{m:Exp Str='pipelinego != null \:pipelinego.yhh\,null'}"
             Tm="0001"
		     Ljgql="{m:Exp Str='pipelinego != null \:pipelinego.f_cumulativepurchase\,null'}"
		     Bkcs="0"
		     Bjql="{m:Exp Str='pipelinego != null \:pipelinego.bjql.ToInt()\,null'}"
		     Czsx="{m:Exp Str='pipelinego != null \:pipelinego.czsx.ToInt()\,null'}"
		     Tzed="{m:Exp Str='pipelinego != null \:pipelinego.tzed.ToInt()\,null'}"	
		     Kmm="{m:Exp Str='pipelinego != null \:pipelinego.kmm\,null'}"
		     Sqrq="{m:Exp Str='SysTime.Now.ToString($yyyyMMdd$)'}"
		     OldPrice="{m:Exp Str='pipelinego != null\:pipelinego.f_gasprice*100\,0'}"
		     NewPrice="{m:Exp Str='pipelinego != null\:pipelinego.f_gasprice*100\,0'}"
		     Sxrq="{m:Exp Str='SysTime.Now.ToString($yyyyMMdd$)'}"
		     Sxbj="{m:Exp Str=card.Factory \=\= $ZhenLan$ \:null\,$1$}"
		    >
            </oo:NewGeneralICCard>
            <!--根据用户档案的计划月份产生抄表单-->
            <!--f_userinfoid\, f_userid\, f_handid\, f_username\, lastinputgasnum\, f_gaswatchbrand\, f_metertype\,
				f_handarea\, f_address\, f_districtname\, f_usertype\, f_gasprice\, f_gaspricetype\, f_dibaohu\, f_apartment\,
				f_phone\, scinputdate\, f_inputtor\, f_yhxz\, f_weizhi\, f_menzhan\,f_stairtype\,
				f_zerenbumen\, f_state\, shifoujiaofei\, users\, f_cusDom\, f_cusDy-->
            <!--删除抄表单-->
            <b:HQLAction Name="DeleteHandplan" WebClientInfo="{StaticResource dbclient}" Type="sql"
			HQL="{m:Exp Str=$delete from t_handplan where f_state \=\'未抄表\' and shifoujiaofei \=\'否\' and f_userid\=\'\{userfiles.f_userid\}\'}" />
            <b:HQLAction Name="InsertHandplan" WebClientInfo="{StaticResource dbclient}" Type="sql"  
			HQL="{m:Exp Str=$insert into t_handplan(f_userid\,f_userinfoid\, f_handid\, f_username\, lastinputgasnum\, f_gaswatchbrand\, f_metertype\,
				f_handarea\, f_address\, f_districtname\, f_usertype\, f_gasprice\, f_gaspricetype\, f_dibaohu\, f_apartment\,
				f_phone\, scinputdate\, f_inputtor\, f_yhxz\, f_weizhi\, f_menzhan\,f_stairtype\,
				f_zerenbumen\, f_state\, shifoujiaofei\, users\, f_cusDom\, f_cusDy\,f_gasmeterstyle )  
                select f_userid\,f_userinfoid\, f_handid\, f_username\, lastinputgasnum\, f_gaswatchbrand\, f_metertype\,
				f_handarea\, f_address\, f_districtname\, f_usertype\, f_gasprice\, f_gaspricetype\, f_dibaohu\, f_apartment\,
				f_phone\, lastinputdate\, f_inputtor\, f_yhxz\, f_weizhi\, f_menzhan\,f_stairtype\,
				f_zerenbumen\, \'未抄表\'\, \'否\'\, id\, f_cusDom\, f_cusDy\,f_gasmeterstyle from t_userfiles where f_gasmeterstyle \!\=\'卡表\' and f_userid\=\'\{userfiles.f_userid\}\'}"/>
            <!-- 最新指数插入记录 -->
            <b:HQLAction Name="Insertcbnum" WebClientInfo="{StaticResource dbclient}" Type="sql" 
			HQL="{m:Exp Str=$insert into t_cbnum(f_userid\, f_gasdate\, f_gasnum\, f_meternumber\, f_gaswatchbrand\,jval\,f_aliasname ) values (
               \'\{pipeline.DataContext.f_userid\}\'\,\'\{pipeline.DataContext.f_cmdate\}\'\,\'\{pipeline.DataContext.f_newgasmeteraccomodations\}\'\,\'\{pipeline.DataContext.f_cmnewmeternumber\}\'\,\'\{pipeline.DataContext.f_cmnewgaswatchbrand\}\'\,\'-0.0001\'\,\'\{pipeline.DataContext.f_aliasname\}\')}"/>
            <!--生成抄表单-->
            <b:BatchExcuteAction Name="CreateHandplan" WebClientInfo="{StaticResource dbclient}">
                <b:BatchInfo Source="{m:Exp Str=DeleteHandplan}" MethodName="InvokeToJson"/>
                <b:BatchInfo Source="{m:Exp Str=InsertHandplan}" MethodName="InvokeToJson"/>
                <b:BatchInfo CanSave="{m:Exp Str=pipelinego.f_newgasmeterstyle\=\=$远传水表$}" Source="{m:Exp Str=Insertcbnum}" MethodName="InvokeToJson"/>
            </b:BatchExcuteAction>

            <!--保存-->
            <b:BatchExcuteAction Name="SaveAction" WebClientInfo="{StaticResource dbclient}" >
                <b:BatchInfo Source="{m:Exp Str=xiaozhang}" MethodName="InvokeToJson"/>
                <b:BatchInfo Source="{m:Exp Str=updateuserfiles}" MethodName="InvokeToJson"/>
            </b:BatchExcuteAction>
        </m:ResourceLoad>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="3*"/>
                <ColumnDefinition Width="7*"/>
            </Grid.ColumnDefinitions>
            <c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=pipelines.ItemsSource.IsBusy}">
                <toolkit:DockPanel >
                    <!--信息列表按钮-->
                    <toolkit:DockPanel  toolkit:DockPanel.Dock="Bottom" Margin="0" Height="45">
                        <!--信息列表翻页-->
                        <TextBlock  TextWrapping="Wrap" Text="{m:Exp Str=$共\{pipelines.ItemsSource.Count\}项}" Width="197" HorizontalAlignment="Stretch" Foreground="#FF140000" VerticalAlignment="Stretch" Margin="5"/>
                    </toolkit:DockPanel>
                    <toolkit:DockPanel x:Name="pipelinesearch" toolkit:DockPanel.Dock="Top" Margin="0,0,0,5" Background="#FFCFEFFF" >
                        <a:Data.Context>
                            <o:SearchObject  />
                        </a:Data.Context>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="80"/>
                                <ColumnDefinition Width="50*"/>
                                <ColumnDefinition Width="80"/>
                                <ColumnDefinition Width="50*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                            </Grid.RowDefinitions>
                            <TextBlock TextWrapping="Wrap" Text="用户编号：" HorizontalAlignment="Right"/>
                            <TextBox TextWrapping="Wrap" Grid.Column="1" Height="25" Text="{m:Bind Path=f_userinfoid}" a:ControlAttach.DefaultButton="searchbutton" Margin="0,2,0,1" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_userinfoid" Operator="f_userinfoid like 'this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock TextWrapping="Wrap" Grid.Column="2" Text="用户姓名：" HorizontalAlignment="Right" Height="28" VerticalAlignment="Top"/>
                            <TextBox TextWrapping="Wrap" Grid.Column="3" Height="25"  Text="{m:Bind Path=f_username}" a:ControlAttach.DefaultButton="searchbutton" Margin="0,2,0,0" VerticalAlignment="Top" d:LayoutOverrides="Height">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_username" Operator="f_username like 'this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock TextWrapping="Wrap" Grid.Row="1" Text="表具编号：" HorizontalAlignment="Right"/>
                            <TextBox TextWrapping="Wrap" Grid.Row="1" Grid.Column="1" Height="25" Text="{m:Bind Path=f_userid}" a:ControlAttach.DefaultButton="searchbutton" Margin="0,2,0,1" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_userid" Operator="f_userid like 'this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock TextWrapping="Wrap" Grid.Row="1" Grid.Column="2" Text="水表钢印号：" HorizontalAlignment="Right" Height="28" VerticalAlignment="Top"/>
                            <TextBox TextWrapping="Wrap" Grid.Column="3" Grid.Row="1" Height="25"  Text="{m:Bind Path=f_meternumber}" a:ControlAttach.DefaultButton="searchbutton" Margin="0,2,0,0" VerticalAlignment="Top" d:LayoutOverrides="Height">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_meternumber" Operator="f_meternumber like 'this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <Button Content="查 询" x:Name="searchbutton" Grid.Column="2" Grid.Row="2" Click="{m:Exp Str=pipelinesearch.DataContext.Search()}" Margin="0,-1,5,0" HorizontalAlignment="Right" Width="50" Height="28" VerticalAlignment="Top"/>
                            <TextBlock TextWrapping="Wrap" Grid.Row="2" Text="地　　址：" HorizontalAlignment="Right" Height="28" VerticalAlignment="Top" Margin="0,2,0,0"/>
                            <TextBox TextWrapping="Wrap" Grid.Row="2" Height="25" Grid.Column="1" Text="{m:Bind Path=f_address}" a:ControlAttach.DefaultButton="searchbutton" Margin="0,2,0,0" VerticalAlignment="Top" d:LayoutOverrides="Height">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_address" Operator="f_address like '%this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                        </Grid>
                    </toolkit:DockPanel>
                    <sdk:DataGrid x:Name="pipelines" Margin="0,0,0,23" ItemsSource="{m:Exp Str=pipelineslist}" 
				SelectedItem="{m:Exp Str=pipelinego.f_userid\=\=null:null\,pipelines.SelectedItem |or| pipelineslist\=>DataLoaded!\=null:null\,null}" 
            		AutoGenerateColumns="False"  IsReadOnly="True">

                        <sdk:DataGrid.Columns>
                            <sdk:DataGridTextColumn Header="序号" Binding="{Binding Index}" d:IsLocked="True"/>
                            <sdk:DataGridTextColumn Header="换表单号" Binding="{Binding f_changemeterid}" d:IsLocked="True"/>
                            <sdk:DataGridTextColumn Header="用户编号" Binding="{Binding f_userinfoid}" d:IsLocked="True"/>
                            <sdk:DataGridTextColumn Header="用户姓名" Binding="{Binding f_username}" d:IsLocked="True"/>
                            <sdk:DataGridTextColumn Header="表具编号" Binding="{Binding f_userid}" d:IsLocked="True"/>
                            <sdk:DataGridTextColumn Header="水表钢印号" Binding="{Binding f_meternumber}" d:IsLocked="True"/>
                            <sdk:DataGridTextColumn Header="地址" Binding="{Binding f_address}" d:IsLocked="True"/>
                            <!--<sdk:DataGridTextColumn Header="卡号" Binding="{Binding f_cardid}" d:IsLocked="True"/>-->
                        </sdk:DataGrid.Columns>
                    </sdk:DataGrid>
                </toolkit:DockPanel >
            </c:BusyIndicatorEx>
            <!--操作区按钮-->
            <c:BusyIndicatorEx x:Name="ui_userfiles" BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=pipeline.DataContext.IsBusy or userfiles.IsBusy or SaveAction.IsBusy}" Grid.Column="1" Margin="8,0,0,0">
                <Grid Background="{x:Null}" x:Name="pipeline" DataContext="{m:Exp Str=pipelinego}" Height="631" VerticalAlignment="Top" >
                    <StackPanel  Orientation="Horizontal" VerticalAlignment="Bottom" Margin="297,0,0,36" Height="23">
                        <!--<Button x:Name="save" Content="保存并发卡" Margin="0" 
            			IsEnabled="{m:Exp Str=data.f_newgasmeterstyle\=\=$卡表$ and !pipeline.DataContext.HasErrors}" 
            			Click="{m:Exp Str=userfiles.f_surplusgas\=pipeline.DataContext.f_cmaddgas;userfiles.f_bugas\=pipeline.DataContext.f_cmaddgas;userfiles.f_metergasnums\=pipeline.DataContext.f_zhi;userfiles.f_times\=0;card.Gas\=0;card.Sqrq\=pipelinego.f_cmdate.ToString($yyyyMMdd$);submit.IsShow\=True}"/>-->
                        <Button x:Name="onlySave" Content=" 保 存 " Click="save_Click"  Margin="20,0,0,0" Width="50"
            			IsEnabled="{m:Exp Str=data.f_newgasmeterstyle!\=$卡表$  and pipelinego.f_oldcurrentbase-pipelinego.lastinputgasnum\>0 and !pipeline.DataContext.HasErrors}"/>
                        <Button Content="撤 销" Margin="20,0,5,0" Click="{m:Exp Str=pipeline.DataContext.New();userfiles.New()}" Width="50"/>
                    </StackPanel>
                    <Grid Margin="0,0,0,59" Grid.Column="0" >
                        <Grid.Background>
                            <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                                <GradientStop Color="#FFF2F8FF" Offset="0"/>
                                <GradientStop Color="#FFDDF2FD" Offset="1"/>
                            </LinearGradientBrush>
                        </Grid.Background>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="110"/>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="110"/>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="110"/>
                            <ColumnDefinition Width="1*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="10"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30" />
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30" />
                            <RowDefinition Height="30" />
                            <RowDefinition Height="30" />
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="10" />
                            <RowDefinition Height="30" />
                            <RowDefinition Height="0" />
                            <RowDefinition Height="0" />
                            <RowDefinition Height="60" />
                            <RowDefinition Height="30" />
                            <RowDefinition Height="30" />
                        </Grid.RowDefinitions>
                        <TextBlock Margin="0" TextWrapping="Wrap" Text="用户编号：" Height="25" TextAlignment="Right"/>
                        <TextBlock Margin="0" TextWrapping="Wrap" Text="用户姓名：" Grid.Column="2" Height="25" TextAlignment="Right"/>
                        <TextBlock Margin="0,5,0,0" TextWrapping="Wrap" Grid.Row="1" Grid.Column="4" Text="换表单号：" Height="25" TextAlignment="Right"/>
                        <TextBlock Margin="0,5,0,0" TextWrapping="Wrap" Grid.Column="4" Text="表具编号：" Height="25" TextAlignment="Right"/>
                        <TextBlock Margin="0" Grid.Row="1" TextWrapping="Wrap" Text="用户地址：" Height="25" TextAlignment="Right"/>
                        <TextBlock Margin="0" Grid.Row="2" TextWrapping="Wrap" Text="抄表编号：" Height="25" TextAlignment="Right"/>
                        <TextBlock Margin="0" Grid.Row="6" TextWrapping="Wrap" Text="旧表止度：" Grid.Column="2" Height="25" TextAlignment="Right"/>
