<?xml version="1.0"  encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
	<!--抄表册-->
	<class entity-name="t_meterchangeline" table="t_meterchangeline" >
		<id name="id" type="int">
			<generator class="identity"></generator>
		</id>
		<!-- 抄表册号-->
		<property name="f_newlistid" type="string"/>
		<!--用户编号-->
		<property name="f_userinfoid" type="string"/>
		<!--表具编号-->
		<property name="f_userid" type="string"/>
		<!--抄表编号-->
		<property name="f_handid" type="string"/>
		<!--用户姓名-->
		<property name="f_username" type="string"/>
		<!--用户地址-->
		<property name="f_address" type="string"/>
		<!--水价类型-->
		<property name="f_stairtype" type="string"/>   
		<!--抄表周期-->
		<property name="f_handzq" type="string"/>
		<!--投线编码-->
		<property name="f_touxianno" type="string"/>
		<!--调线次序号-->
		<property name="f_newhandno" type="string"/>
		<!--调线编码-->
		<property name="f_newtiaoxianno" type="string"/>
		<!--分公司-->
		<property name="f_filiale" type="string"/>
		<!--操作员-->
		<property name="f_cmoperator" type="string"/>
		<!--调线原因-->
		<property name="f_changelinenote" type="string"/>
	</class>
</hibernate-mapping>